d item from searched list to products list\r\n  const handleProducts = (product) => {\r\n    let newProducts = products;\r\n    newProducts.unshift(product);\r\n    setProducts(newProducts);\r\n    setInputValue(\"\");\r\n\r\n    //Filtring Searched List when pick one products\r\n    let newData = searchProducts;\r\n    newData = newData.filter((elem) => elem.name !== product.name);\r\n    setSearchProducts(newData);\r\n  };\r\n\r\n  //Removing item from products List\r\n  const removeItem = (product) => {\r\n    let newProducts = products;\r\n    newProducts = newProducts.filter((elem) => elem.name !== product.name);\r\n    setProducts(newProducts);\r\n\r\n    //Return removing item from products to searched list\r\n    let newSearched = searchProducts;\r\n    newSearched.unshift(product);\r\n    setSearchProducts(newSearched);\r\n  };\r\n\r\n  //Change quantity of each products\r\n  const handleQuantity = (product, operation) => {\r\n    const newProducts = products.map((elem) => {\r\n      if (operation === \"+\") {\r\n        if (elem.name === product.name) {\r\n          return {\r\n            ...elem,\r\n            quantity: elem.quantity + 1,\r\n          };\r\n        }\r\n      } else if (operation === \"-\") {\r\n        if (elem.name === product.name) {\r\n          if (elem.quantity > 1) {\r\n            return {\r\n              ...elem,\r\n              quantity: elem.quantity - 1,\r\n            };\r\n          }\r\n        }\r\n      }\r\n      return {\r\n        ...elem,\r\n      };\r\n    });\r\n    setProducts(newProducts);\r\n  };\r\n\r\n  //Create Receipt\r\n  const handleReceipt = () => {\r\n    console.log(products);\r\n    createReceipt(products)\r\n      .then(() => console.log(\"Succefuly create receipt of products\"))\r\n      .catch((err) => \"Cannot create receipt of products\" + err);\r\n  };\r\n\r\n  // PROVERA ZA RECEPTE\r\n  // useEffect(() => {\r\n  // getReceipts()\r\n  // .then((snapShot) => {\r\n  //  if (!snapShot.empty) {\r\n  //  snapShot.docs.forEach((doc, i) => {\r\n  // console.log(doc.id);\r\n  // console.log(doc.data());\r\n  // const createdAt = doc.data().createdAt;\r\n  // console.log(createdAt);\r\n  // const date = createdAt.toDate();\r\n  // console.log(date.toLocaleTimeString());\r\n  // });\r\n  //       }\r\n  //     })\r\n  //     .catch((err) => console.log(err));\r\n  // }, []);\r\n\r\n  return (\r\n    <main className=\"home\">\r\n      <div className=\"wrapper\">\r\n        <div className=\"search-wrapper\">\r\n          <label>Search for food</label>\r\n          <input\r\n            type=\"text\"\r\n            placeholder=\"Enter food name...\"\r\n            onChange={handleChange}\r\n            value={inputValue}\r\n          />\r\n        </div>\r\n        <div className=\"lists-wrapper\">\r\n          {inputValue ? (\r\n            <SearchedUL\r\n              filterProducts={filterProducts}\r\n              handleProducts={handleProducts}\r\n            />\r\n          ) : (\r\n            <ProductUL\r\n              products={products}\r\n              removeItem={removeItem}\r\n              handleQuantity={handleQuantity}\r\n            />\r\n          )}\r\n        </div>\r\n        <div className=\"button-wrapper\">\r\n          {products.length > 0 && (\r\n            <button className=\"checkout-btn\">Done</button>\r\n          )}\r\n        </div>\r\n      </div>\r\n    </main>\r\n  );\r\n};\r\n\r\nexport default Home;\r\n","C:\\Users\\m_uro\\Desktop\\Alas\\test-alas\\src\\services\\fire.js",[],"C:\\Users\\m_uro\\Desktop\\Alas\\test-alas\\src\\services\\firestore.js",[],"C:\\Users\\m_uro\\Desktop\\Alas\\test-alas\\src\\components\\Header\\Menu\\index.js",[],"C:\\Users\\m_uro\\Desktop\\Alas\\test-alas\\src\\components\\SearchedUL\\index.js",[],"C:\\Users\\m_uro\\Desktop\\Alas\\test-alas\\src\\components\\ProductUL\\index.js",[],"C:\\Users\\m_uro\\Desktop\\Alas\\test-alas\\src\\components\\ProductUL\\ProductLI\\index.js",[],"C:\\Users\\m_uro\\Desktop